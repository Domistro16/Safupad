# Network Configuration
# Note: The SDK automatically uses the network selected in the user's wallet (RainbowKit)
# Supported networks: BSC Mainnet (56) and BSC Testnet (97)
# When no wallet is connected, SDK defaults to BSC Testnet (97)
NEXT_PUBLIC_NETWORK=bscTestnet
NEXT_PUBLIC_CHAIN_ID=97
NEXT_PUBLIC_RPC_URL=https://data-seed-prebsc-1-s1.binance.org:8545
NEXT_PUBLIC_CHAIN_NAME=BSC Testnet

# Contract Addresses
NEXT_PUBLIC_PLATFORM_WALLET=0x0000000000000000000000000000000000000001
NEXT_PUBLIC_ACADEMY_WALLET=0x0000000000000000000000000000000000000002
NEXT_PUBLIC_INFOFI_WALLET=0x0000000000000000000000000000000000000003

# Factory Contract Addresses (deploy these first)
NEXT_PUBLIC_PROJECT_RAISE_FACTORY=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_INSTANT_LAUNCH_FACTORY=0x0000000000000000000000000000000000000000

# SAFU Domain Name Service Contract Addresses
# These enable .safu domain name resolution in wallet addresses
NEXT_PUBLIC_SAFU_REGISTRY=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_SAFU_REVERSE_REGISTRAR=0x0000000000000000000000000000000000000000

# WalletConnect Project ID
# Get yours at https://cloud.walletconnect.com
NEXT_PUBLIC_WALLETCONNECT_PROJECT_ID=YOUR_PROJECT_ID

# API Keys
NEXT_PUBLIC_BSCSCAN_API_KEY=your_bscscan_api_key
NEXT_PUBLIC_COINGECKO_API_KEY=your_coingecko_api_key
NEXT_PUBLIC_ALCHEMY_API_KEY=your-alchemy-api-key-here

# PancakeSwap Router (for graduation)
NEXT_PUBLIC_PANCAKESWAP_ROUTER=0x10ED43C718714eb63d5aA57B78B54704E256024E

# Admin Addresses (comma-separated)
NEXT_PUBLIC_ADMIN_ADDRESSES=0x0000000000000000000000000000000000000000

# Feature Flags
NEXT_PUBLIC_ENABLE_TESTNET=false
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_TWEET_BOT=false

# Social Links
NEXT_PUBLIC_TWITTER_URL=https://twitter.com/infofi
NEXT_PUBLIC_TELEGRAM_URL=https://t.me/infofi
NEXT_PUBLIC_DISCORD_URL=https://discord.gg/infofi
NEXT_PUBLIC_DOCS_URL=https://docs.infofi.io

# Application Settings
NEXT_PUBLIC_APP_NAME=InfoFi Launch
NEXT_PUBLIC_APP_DESCRIPTION=The most advanced token launchpad on BSC
NEXT_PUBLIC_APP_URL=https://launch.infofi.io

NEXT_PUBLIC_UPLOAD_KEY=c200c837eef6d9aec9dd00ac
